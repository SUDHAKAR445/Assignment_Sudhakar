package tryOut;

class Test1
{
	static class Run
	{
		int a = 0;
		int test()
		{
			this.a = a++;
			return a;
		}
		
	}
}
public class StaticInc {
    public static void main(String args[])
    {
    	Test1.Run s1 = new Test1.Run();
    	System.out.println(s1.test());
    	System.out.println(s1.test());
    }
}

