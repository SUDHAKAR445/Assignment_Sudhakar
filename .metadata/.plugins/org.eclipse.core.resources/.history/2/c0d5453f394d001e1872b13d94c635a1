package com.blackJackGame;
import java.util.*;

public enum Suit 
{
	// Suits along their values
		//	CLUB("Clubs"),
		//	DIAMOND("Diamonds"),
		//	HEART("Hearts"),
		//	SPADE("Spades");
    CLUB("Clubs"),
    DIAMOND("Diamonds"),
    HEART("Hearts"),
    SPADE("Spades");

	String suitName;
	
	// set the suit name to suitName variable
	Suit (String suitName)
	{
		this.suitName = suitName;
	}
	
	//To return the name of the suit
	
	public String toString()
	{
		return suitName;
	}
}
